<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<SplitPane dividerPositions="0.3" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SearchPane">
   <items>
      <AnchorPane prefHeight="500.0" prefWidth="200.0">
          <children>
              <VBox prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                  <children>
                      <Label text="This is Dictionary" />
                      <HBox prefHeight="31.0" prefWidth="200.0">
                          <children>
                              <TextField fx:id="searchTextField" onKeyReleased="#handleChangeInput" />
                              <Button fx:id="searchButton" mnemonicParsing="false" onAction="#clickSearch" text="S" />
                          </children>
                      </HBox>
                      <ListView fx:id="hintWordsView" onMouseClicked="#clickWordInListView" prefHeight="340.0" prefWidth="200.0" />
                  </children>
              </VBox>
          </children>
          <padding>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
          </padding>
      </AnchorPane>
      <AnchorPane SplitPane.resizableWithParent="false">
         <children>
            <VBox prefHeight="200.0" prefWidth="400.0" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0">
               <children>
                  <HBox alignment="CENTER" spacing="20.0">
                     <children>
                        <Button fx:id="upgradeButton" mnemonicParsing="false" onAction="#clickUpgradeButton" text="Upgrade" />
                        <Button fx:id="deleteButton" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#clickDeleteButton" text="Delete" />
                        <Button fx:id="addButton" layoutX="62.0" layoutY="10.0" mnemonicParsing="false" onAction="#clickAddButton" text="Add" />
                     </children>
                  </HBox>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label fx:id="spellingLabel" text="Word name" />
                        <Label fx:id="pronunciationLabel" text="Pronunciation" />
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <Button fx:id="playAudioButton" mnemonicParsing="false" onAction="#clickPlayAudio" text="Audio" />
                              <Button fx:id="favoriteButton" mnemonicParsing="false" text="Favorite" />
                           </children>
                        </HBox>
                        <Label fx:id="meaningLabel" layoutX="10.0" layoutY="10.0" text="meaning" />
                     </children>
                  </VBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </items>
</SplitPane>
