<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.EditPane">
   <children>
      <HBox alignment="CENTER" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Button fx:id="upgradeButton" mnemonicParsing="false" onAction="#upgradeWord" text="Upgrade" />
            <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteWord" text="Delete" />
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addWord" text="Add" />
         </children>
      </HBox>
   </children>
</AnchorPane>
